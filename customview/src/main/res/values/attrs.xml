<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomRectView">
        <attr name="custom_rect_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomGroupWidgetTitleBar">
        <attr name="title_text_color" format="color" />
        <attr name="title_bar_background" format="color" />
        <attr name="title_text_content" format="string" />
    </declare-styleable>

    <!--声明属性-->
    <declare-styleable name="MyCustomProgress">
        <attr name="roundColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="roundWidth" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="maxProgress" format="integer" />
        <attr name="isStroke" format="boolean" />
        <attr name="isTextDisplay" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="integer" />
    </declare-styleable>
</resources>
